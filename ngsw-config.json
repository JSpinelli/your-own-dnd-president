{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name":"candidates",
      "urls":[
        "https://your-own-dnd-president.firebaseio.com/*"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "10m",
        "timeout": "10s",
        "strategy": "performance"
      }
    }

  ]
}
